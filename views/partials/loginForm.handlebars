<form method="post" class="register-form" autocomplete="off">
  <div class="input_holder">
    <input type="text" name="email"> E-mail Address
  </div>
  <div class="input_holder">
    <input type="password" name="password"> Password
  </div>
  <input type="hidden" name="_csrf" value="{{csrfToken}}">
  <button name="registerButton" class="soundEffect">Log In</button>
  <a class="backToReg" href="/register">Back to Registration</a>
</form>
